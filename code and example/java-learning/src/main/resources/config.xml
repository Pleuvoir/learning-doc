<?xml version="1.0" encoding="UTF-8"?>
<config>
	
	<mode>debug</mode>

	<!-- 缓存 -->
	<cache>
		<!-- 是否开启本地持久化 -->
		<persist>true</persist>
		<!-- 持久化文件位置 -->
		<location>d:/cache</location>
	</cache>
	
	<dbTypes>
		<dbType>
			<name>oracle</name>
		</dbType>
		
		<dbType>
			<name>mysql</name>
		</dbType>
	</dbTypes>
</config>